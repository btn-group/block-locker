{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ReceiveAnswer",
  "anyOf": [
    {
      "type": "object",
      "required": [
        "create_or_update_locker"
      ],
      "properties": {
        "create_or_update_locker": {
          "type": "object",
          "required": [
            "status",
            "user_locker"
          ],
          "properties": {
            "status": {
              "$ref": "#/definitions/ResponseStatus"
            },
            "user_locker": {
              "$ref": "#/definitions/UserLocker"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "get_user_locker"
      ],
      "properties": {
        "get_user_locker": {
          "type": "object",
          "required": [
            "status",
            "user_locker_response"
          ],
          "properties": {
            "status": {
              "$ref": "#/definitions/ResponseStatus"
            },
            "user_locker_response": {
              "$ref": "#/definitions/UserLockerResponse"
            }
          }
        }
      }
    }
  ],
  "definitions": {
    "ResponseStatus": {
      "type": "string",
      "enum": [
        "Success"
      ]
    },
    "UserLocker": {
      "type": "object",
      "required": [
        "content",
        "whitelisted_addresses"
      ],
      "properties": {
        "content": {
          "type": "string"
        },
        "whitelisted_addresses": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "UserLockerResponse": {
      "type": "object",
      "required": [
        "content"
      ],
      "properties": {
        "content": {
          "type": "string"
        },
        "whitelisted_addresses": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "type": "string"
          }
        }
      }
    }
  }
}
